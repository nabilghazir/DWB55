    <h1 class="container-fluid text-center">My Project</h1>
    <div class="container">
      <div class="row">
        {{#each data}}
          <div class="col-md-4 my-3">
            <div class="card" style="width: 100%;">
              <div class="card-body">
                <img
                  src="https://avatars3.githubusercontent.com/u/34464790?s=220&v=4"
                />
                <h3 class="card-title">
                  <a href="/myproject-detail/{{this.id}}">
                    {{projectname}}
                  </a>
                </h3>
                <h5 class="card-title">
                  <p style="display:none">
                    {{@index}}
                  </p>
                  Durasi:
                  {{startdate}}
                  -
                  {{enddate}}
                </h5>
                <p class="card-text">{{description}}</p>
                <div class="d-flex flex-row">
                  <div class="technologies">
                    {{#if this.technologies.nodejs}}
                      <i class="fab fa-node-js"></i>
                      Node.js
                    {{/if}}
                    {{#if this.technologies.javascript}}
                      <i class="fab fa-js"></i>
                      JavaScript
                    {{/if}}
                    {{#if this.technologies.reactjs}}
                      <i class="fab fa-react"></i>
                      React
                    {{/if}}
                    {{#if this.technologies.android}}
                      <i class="fab fa-android"></i>
                      Android
                    {{/if}}
                  </div>
                </div>
              </div>
              <div class="p-3 d-flex flex-row justify-content-between">
                <a
                  href="/edit-project/{{@index}}"
                  class="btn btn-secondary mr-2 h-25"
                >
                  Edit
                </a>
                <form method="post" action="/delete-project/{{@index}}">
                  <button type="submit" class="btn btn-secondary">
                    Delete
                  </button>
                </form>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
